{
  "compilerOptions": {
    "lib": [
      "es6"
    ],
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "rootDir": ".",
    "outDir": "build/compiled",
    "declaration": true
  },
  "exclude": [
    "build",
    "node_modules"
  ]
}